pipeline {
    agent any

    stages {
        stage('install') {
            steps {
                sh 'npm install'
            }
        }
    stage('build') {
            steps {
                sh 'npm run build'
            }
        }
    stage('depoly') {
            steps {
                sh 'sudo cp -r dist/sai-app/* /var/www/html/'
            }
        }
    }
}
